<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:view="clr-namespace:Check.SPort.View"
                    xmlns:vm="clr-namespace:Check.SPort.ViewModel">

    <DataTemplate DataType="{x:Type vm:MainViewModel}">
        <view:MainView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ProtocolCustomVM}">
        <view:ProtocolCustomView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ProtocolXonXoffVM}">
        <view:ProtocolXonXoffView />
    </DataTemplate>
    
</ResourceDictionary>