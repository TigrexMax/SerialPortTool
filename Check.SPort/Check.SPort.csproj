<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>WinExe</OutputType>
		<TargetFramework>net8.0-windows10.0.22621.0</TargetFramework>
		<Nullable>disable</Nullable>
		<UseWPF>true</UseWPF>
		<SupportedOSPlatformVersion>10.0.22621.0</SupportedOSPlatformVersion>
		<UseWindowsForms>True</UseWindowsForms>
		<AssemblyName>ProtocolConnection</AssemblyName>
		<AssemblyVersion>2.0.4</AssemblyVersion>
		<FileVersion>2.0.4</FileVersion>
		<ApplicationIcon>Resources\Image\Icon_Tool_2.ico</ApplicationIcon>
		<Version>2.0.4</Version>
		<PackageIcon>Icon_Tool_2.png</PackageIcon>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<NeutralLanguage>it</NeutralLanguage>
		<Authors>Francesco Rubino</Authors>
	</PropertyGroup>

	<ItemGroup>
		<None Remove="Resources\DLL\CustomEngineering.CeFCom.dll" />
		<None Remove="Resources\Image\home_sfondo_2.png" />
		<None Remove="Resources\Image\Icon_Tool_2.ico" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="MaterialDesignThemes" Version="5.0.0" />
		<PackageReference Include="System.IO.Ports" Version="8.0.0" />
	</ItemGroup>

	<ItemGroup>
		<Content Include="Resources\DLL\CustomEngineering.CeFCom.dll">
		  <CopyToOutputDirectory>Always</CopyToOutputDirectory>
		</Content>
		<Content Include="Resources\Image\home_sfondo_2.png">
		  <CopyToOutputDirectory>Always</CopyToOutputDirectory>
		</Content>
		<Content Include="Resources\Image\Icon_Tool_2.ico">
		  <CopyToOutputDirectory>Always</CopyToOutputDirectory>
		</Content>
		<Content Include="Resources\Image\Icon_Tool_2.png">
		  <CopyToOutputDirectory>Always</CopyToOutputDirectory>
		</Content>
	</ItemGroup>

	<ItemGroup>
	  <None Include="..\README.md">
	    <Pack>True</Pack>
	    <PackagePath>\</PackagePath>
	  </None>
	</ItemGroup>

	<ItemGroup>
	  <Reference Include="CustomEngineering.CeFCom">
	    <HintPath>Resources\DLL\CustomEngineering.CeFCom.dll</HintPath>
	  </Reference>
	</ItemGroup>

	<ItemGroup>
	  <None Update="Resources\Image\Icon_Tool_2.png">
	    <Pack>True</Pack>
	    <PackagePath>\</PackagePath>
	  </None>
	</ItemGroup>

	<ItemGroup>
	  <Page Update="Styles\ComboBoxs.xaml">
	    <CopyToOutputDirectory></CopyToOutputDirectory>
	  </Page>
	</ItemGroup>

</Project>
